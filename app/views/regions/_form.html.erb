
<%= form_with model: @region, local: true do |f| %>

    <%= f.label :name %>
    <br>
    <%= f.text_field :name, required: true %>
    <br><br>


    <%= f.label :country %>
    <br>
    <%= f.text_field :country, required: true %>
    <br><br>


    <%= f.label :image, "Image URL" %>
    <br>
    <%= f.text_field :image %>
    <br><br>

    <%= f.submit %>


<% end %>